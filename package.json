{
  "name": "sketch-tailwind",
  "description": "Export your design to a theme-file you can use in your Tailwind Css config.",
  "version": "2.1.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-tailwind",
    "manifest": "src/manifest.json",
    "main": "sketch-tailwind.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "url": "https://github.com/jan-dh/sketch-tailwind"
  },
  "license": "MIT",
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@skpm/builder": "^0.8.0",
    "@skpm/extract-loader": "^2.0.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^6.7.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "html-loader": "^0.5.1",
    "prism-react-renderer": "^0.1.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.3.1"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@skpm/fs": "^0.2.5",
    "sketch-module-web-view": "^3.4.1",
    "util": "^0.11.1"
  },
  "author": "Jan D'Hollander<jan.dhollander84@gmail.com>"
}
